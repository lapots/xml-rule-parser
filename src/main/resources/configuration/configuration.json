{
  "default_package" : "com.lapots.breed.judge.domain.rule",
  "class_template"  : "./template/rule_class_template.template",
  "bindings": {
    "and" : "&&",
    "or"  : "||"
  },
  "conditions": {
    "not_equals"        : "!=",
    "less_than"         : "<",
    "equals"            : "==",
    "more_than"         : ">",
    "less_equal_than"   : "<=",
    "more_equal_than"   : ">="
  }
}
